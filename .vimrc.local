"autocmd MyAutoCmd BufNewFile,BufRead $HOME/sample/*.php let g:vim_tags_project_tags_command = 'ctags --languages=php --exclude=*.js -f $HOME/.vim/tags/sample.tags $HOME/sample >/dev/null 2>&1'
"autocmd MyAutoCmd BufNewFile,BufRead $HOME/sample/*.php setlocal tags=$HOME/.vim/tags/sample.tags
"autocmd MyAutoCmd BufNewFile,BufRead $HOME/sample/app/views/**/*.php setlocal filetype=html
